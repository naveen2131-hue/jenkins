kind: StorageClass
apiVersion: storage.k8s.io/v1
metadata:
  name: jenkins-pv-volume
  namespace: devops
provisioner: kubernetes.io/azure-file
mountOptions:
  - dir_mode=0754
  - file_mode=0754
  - uid=1000
  - gid=1000
  - mfsymlinks
  - cache=strict
  - actimeo=30
#  persistentVolumeReclaimPolicy: Retain
parameters:
  skuName: Standard_LRS

